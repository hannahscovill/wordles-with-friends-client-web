{
  "app": "npx ts-node --prefer-ts-exts bin/infra.ts",
  "watch": {
    "include": ["**"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/core:explicitStackTags": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": false,
    "@aws-cdk/aws-s3:publicAccessBlockedByDefault": true,
    "@aws-cdk/aws-s3:setUniqueReplicationRoleName": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/custom-resources:logApiResponseDataPropertyTrueDefault": false,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "githubOrg": "hannahscovill",

    "issueProxy": {
      "ssmPrefix": "/wordles/issue-proxy",
      "ssmParams": {
        "githubAppId": "github-app-id",
        "githubPrivateKey": "github-app-private-key",
        "githubInstallationId": "github-installation-id",
        "turnstileSecretKey": "turnstile-secret-key"
      },
      "functionName": "wordles-issue-proxy",
      "githubRepo": "hannahscovill/wordles-with-friends-client-web",
      "allowedOrigins": ["https://wordles.dev", "http://localhost:3000"],
      "turnstileVerifyUrl": "https://challenges.cloudflare.com/turnstile/v0/siteverify",
      "memorySize": 128,
      "timeoutSeconds": 10,
      "rustLogLevel": "info"
    }
  }
}
